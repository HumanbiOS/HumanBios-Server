humanbiosbots.com {
    reverse_proxy humanbios-fb:8443
}

server.humanbiosbots.com {
    handle /files/* {
        root * files
        file_server
    }
    handle /static/* {
        root * static
        file_server
    }
    handle {
        reverse_proxy humanbios-server:8080
    }
}

